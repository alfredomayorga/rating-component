{
  "name": "next13-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "^1.22.19"
  },
 "scripts": {
    "dev": "concurrently \"next dev --turbo\" \"tailwindcss -i styles/globals.css -o styles/dist.css -w\"",
    "build": "tailwindcss -m -i styles/globals.css -o styles/dist.css && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx cypress run",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@next/font": "^13.0.7",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1",
    "next": "13.0.2",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.2.0",
    "concurrently": "^7.6.0",
    "cypress": "^10.11.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.4"
  }
}
